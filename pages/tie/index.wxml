<!--  -->
<swiper indicator-dots="true" autoplay="true" interval="{{interval}}" duration="{{duration}}" class="index-swiper">
  <block wx:for="{{imgUrls}}">
    <swiper-item>
      <image src="{{item}}" class="index-swiper-image" />
    </swiper-item>
  </block>
</swiper>
<!--  -->
<view class="tie-list">
  <view class="tie-list-box" wx:for="{{dataList}}" wx:for-index="index" wx:for-item="item">
    <view class="font-size28">{{item.title}}{{index}}</view>
    <view class="hui-color font-size22">{{item.eTitle}}</view>
  </view>
</view>
 